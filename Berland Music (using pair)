#include<bits/stdc++.h>
using namespace std;
 
int main(){
    int t;
    cin >> t;
    while(t--){
        int n;
        cin >> n;
        int a[n];
        for(int i = 0; i < n; i++){
            cin >> a[i];
        }
        string b;
        cin >> b;
        int j = 0, k = 0;
        pair<int, int> cnt[n];
        pair<int, int> hd[n];
        for(int i = 0; i < n; i++){
            if(b[i] == '0'){
                j++;
                cnt[j].first = a[i];
                cnt[j].second = i;
            }
            else if(b[i] == '1'){
                k++;
                hd[k].first = a[i];
                hd[k].second = i;
            }
        }
        sort(cnt+1, cnt+j+1);
        sort(hd+1, hd+k+1);
        for(int i = 1; i <= j; i++){
            a[cnt[i].second] = i;
        }
         for(int i = 1; i <= k; i++){
            a[hd[i].second] = j+i;
        }
        for(int i = 0; i < n; i++){
            cout << a[i] << " ";
        }
        cout << endl;
    }
    return 0;
}
