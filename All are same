#include<bits/stdc++.h>
using namespace std;
 
int main(){
    int t;
    cin >> t;
    while(t--){
        int n;
        cin >> n;
        int a[n];
       for(int i = 0; i < n; i++){
           cin >> a[i];
       }
       int check = 0;
       int min = a[0];
       for(int i = 1; i < n; i++){
           if(a[i] < min) min = a[i];
           if(a[i] == a[i-1]) check++;
       }
       if(check == n-1) cout << "-1" << endl;
       else{
       int ans = 0;
       for(int i = 0; i < n; i++){
           ans = gcd(ans, a[i]-min);   
       }
       cout << ans << endl;
    }
    }
    return 0;
}
