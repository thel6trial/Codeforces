#include<iostream>
using namespace std;
 
int main(){
    int t;
    cin >> t;
    while(t--){
       int w, h;
       cin >> w>> h;
       int k[4];
       int x[4][200001];
       for(int i = 0; i < 4; i++){
            cin >> k[i];
            for(int j = 0; j < k[i]; j++){
                cin >> x[i][j];
            }
       }
       long long max = 0;
       for(int i = 0; i < 2; i++){
           long long m = (long long) (x[i][k[i]-1] - x[i][0]) * h;
           if(m > max) max = m;
       }
       for(int i = 2; i < 4; i++){
           long long m = (long long) (x[i][k[i]-1] - x[i][0]) * w;
           if(m > max) max = m;
       }
       cout << max << endl;
    }
    return 0;
}
