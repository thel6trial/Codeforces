#include<iostream>
#include<map>
#include<vector>
using namespace std;
 
int main(){
    int t;
    cin >> t;
    while(t--){
        int n;
        cin >> n;
        int a[n];
        for(int i = 1; i <= n; i++){
            cin >> a[i];
        }
        int min = n;
        map<int, int> k;
        for(int i = 1; i <= n; i++){
            if(k[a[i]] == 0){
                k[a[i]] = i;
            }else{
                int z = i - k[a[i]];
                if(z < min) min = z;
            }
        }
        if(min == n) cout << "-1"<<endl;
        else cout << n-min << endl;
    }
    return 0;
}
